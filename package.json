{
  "name": "all-of-just",
  "version": "0.0.5",
  "description": "A single collection of all of Just utility functions in one single library",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./array": {
      "import": "./dist/arrays.js",
      "require": "./dist/arrays.cjs"
    },
    "./collection": {
      "import": "./dist/collection.js",
      "require": "./dist/collection.cjs"
    },
    "./functions": {
      "import": "./dist/functions.js",
      "require": "./dist/functions.cjs"
    },
    "./numbers": {
      "import": "./dist/numbers.js",
      "require": "./dist/numbers.cjs"
    },
    "./objects": {
      "import": "./dist/objects.js",
      "require": "./dist/objects.cjs"
    },
    "./statistics": {
      "import": "./dist/statistics.js",
      "require": "./dist/statistics.cjs"
    },
    "./strings": {
      "import": "./dist/strings.js",
      "require": "./dist/strings.cjs"
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "rimraf ./dist/**/* && cd scripts && tsm compile.ts && cd .. && tsc"
  },
  "keywords": [
    "just",
    "utility",
    "lodash",
    "merge",
    "clone",
    "array",
    "tiny"
  ],
  "author": "PuruVJ",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/rimraf": "^3.0.2",
    "brotli-size": "^4.0.0",
    "esbuild": "^0.13.4",
    "rimraf": "^3.0.2",
    "tsm": "^2.1.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "just-camel-case": "^4.0.2",
    "just-capitalize": "^1.0.1",
    "just-cartesian-product": "^1.0.2",
    "just-clamp": "^3.2.1",
    "just-clone": "^3.2.1",
    "just-compact": "^2.2.0",
    "just-compare": "^1.4.0",
    "just-compose": "^1.1.0",
    "just-curry-it": "^3.2.1",
    "just-debounce-it": "^1.5.0",
    "just-demethodize": "^2.1.0",
    "just-diff": "^3.1.1",
    "just-diff-apply": "^3.0.0",
    "just-entries": "^1.1.0",
    "just-extend": "^4.2.1",
    "just-filter-object": "^1.3.0",
    "just-flatten-it": "^3.2.0",
    "just-flip": "^1.1.0",
    "just-flip-object": "^1.1.0",
    "just-flush": "^1.1.0",
    "just-group-by": "^1.1.1",
    "just-index": "^2.2.1",
    "just-insert": "^2.2.1",
    "just-intersect": "^3.2.0",
    "just-is-circular": "^1.1.0",
    "just-is-empty": "^2.0.1",
    "just-is-prime": "^1.1.1",
    "just-is-primitive": "^1.1.0",
    "just-kebab-case": "^1.1.0",
    "just-last": "^2.2.0",
    "just-left-pad": "^2.1.0",
    "just-map-keys": "^1.1.0",
    "just-map-object": "^1.2.0",
    "just-map-values": "^1.2.1",
    "just-mean": "^1.1.0",
    "just-median": "^1.0.3",
    "just-memoize": "^1.1.0",
    "just-memoize-last": "^1.0.0",
    "just-merge": "^1.2.0",
    "just-mode": "^1.0.0",
    "just-modulo": "^1.2.1",
    "just-omit": "^1.2.0",
    "just-once": "^1.2.0",
    "just-partial-it": "^2.1.1",
    "just-partition": "^1.2.0",
    "just-pascal-case": "^1.1.0",
    "just-percentile": "^1.0.1",
    "just-permutations": "^1.0.1",
    "just-pick": "^2.2.1",
    "just-pluck-it": "^1.2.0",
    "just-prune": "^1.1.0",
    "just-random": "^2.1.1",
    "just-range": "^2.2.0",
    "just-reduce-object": "^1.1.0",
    "just-remove": "^2.1.1",
    "just-replace-all": "^1.0.0",
    "just-right-pad": "^2.1.0",
    "just-safe-get": "^2.1.2",
