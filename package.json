		"url": "https://github.com/puruvj/all-of-just/issues"
	},
	"sideEffects": false,
	"main": "index.js",
	"module": "index.js",
	"types": "index.d.ts",
	"scripts": {
		"pub": "pnpm build && cd dist && pnpm publish",
		"build": "tsc && tsup && cd scripts && tsx post.ts"
	},
	"exports": {
		".": {
			"types": "./index.d.ts",
			"module": "./index.js",
			"import": "./index.js"
		},
		"./arrays": {
			"types": "./arrays.d.ts",
