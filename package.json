{
  "name": "all-of-just",
  "private": true,
  "version": "0.12.0",
  "description": "A single collection of all of Just utility functions in one single library",
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puruvj/all-of-just.git"
  },
  "bugs": {
    "url": "https://github.com/puruvj/all-of-just/issues"
  },
  "sideEffects": false,
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "pub": "pnpm build && cd dist && pnpm publish",
    "build": "cd scripts && tsm cleandir.ts && cd .. && tsc && rollup -c && cd scripts && tsm post.ts"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./arrays": {
      "types": "./arrays.d.ts",
      "module": "./arrays.js",
      "import": "./arrays.js",
      "require": "./arrays.cjs"
    },
    "./collection": {
      "types": "./collection.d.ts",
      "module": "./collection.js",
      "import": "./collection.js",
      "require": "./collection.cjs"
    },
    "./functions": {
      "types": "./functions.d.ts",
      "module": "./functions.js",
      "import": "./functions.js",
      "require": "./functions.cjs"
    },
    "./numbers": {
      "types": "./numbers.d.ts",
      "module": "./numbers.js",
      "import": "./numbers.js",
      "require": "./numbers.cjs"
    },
    "./objects": {
      "types": "./objects.d.ts",
      "module": "./objects.js",
      "import": "./objects.js",
      "require": "./objects.cjs"
    },
    "./statistics": {
      "types": "./statistics.d.ts",
      "module": "./statistics.js",
      "import": "./statistics.js",
      "require": "./statistics.cjs"
    },
    "./strings": {
      "types": "./strings.d.ts",
      "module": "./strings.js",
      "import": "./strings.js",
      "require": "./strings.cjs"
    }
  },
  "keywords": [
    "just",
    "utility",
    "lodash",
    "merge",
    "clone",
    "array",
    "tiny"
  ],
  "author": "PuruVJ (@puruvjdev)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.4.1",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "tslib": "^2.3.1",
    "tsm": "^2.1.4",
    "type-fest": "^2.5.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "just-camel-case": "^5.1.1",
    "just-capitalize": "^2.2.1",
    "just-cartesian-product": "^3.1.1",
    "just-clamp": "^3.3.1",
    "just-clone": "^4.1.1",
    "just-compact": "^2.4.1",
    "just-compare": "^1.5.1",
    "just-compose": "^1.2.2",
    "just-curry-it": "^4.1.1",
    "just-debounce-it": "^2.1.1",
    "just-demethodize": "^2.2.1",
    "just-diff": "^4.1.2",
    "just-diff-apply": "^3.1.2",
    "just-entries": "^1.2.1",
    "just-extend": "^5.1.1",
    "just-filter-object": "^2.1.1",
    "just-flatten-it": "^4.2.1",
    "just-flip": "^1.2.1",
    "just-flip-object": "^1.3.0",
    "just-flush": "^1.2.1",
    "just-group-by": "^1.3.2",
    "just-index": "^3.2.1",
    "just-insert": "^2.4.1",
    "just-intersect": "^3.4.1",
    "just-is-circular": "^1.3.0",
    "just-is-empty": "^2.1.1",
    "just-is-prime": "^1.2.1",
    "just-is-primitive": "^1.2.1",
    "just-kebab-case": "^3.1.1",
    "just-last": "^2.4.1",
    "just-left-pad": "^2.4.0",
    "just-map-keys": "^1.2.1",
    "just-map-object": "^1.3.1",
    "just-map-values": "^2.2.1",
    "just-mean": "^1.3.1",
    "just-median": "^1.2.1",
    "just-memoize": "^1.3.2",
    "just-memoize-last": "^2.1.1",
    "just-merge": "^2.1.1",
    "just-mode": "^1.2.1",
    "just-modulo": "^1.3.1",
    "just-omit": "^1.4.1",
    "just-once": "^1.4.2",
    "just-partial-it": "^2.2.1",
    "just-partition": "^1.4.1",
    "just-pascal-case": "^2.1.1",
    "just-percentile": "^2.1.1",
    "just-permutations": "^1.2.1",
    "just-pick": "^3.1.1",
    "just-pluck-it": "^1.3.1",
    "just-prune": "^1.3.0",
    "just-random": "^2.2.2",
    "just-range": "^3.1.0",
    "just-reduce-object": "^1.2.1",
    "just-remove": "^2.2.1",
    "just-replace-all": "^1.2.1",
    "just-right-pad": "^2.3.0",
    "just-safe-get": "^3.1.1",
    "just-safe-set": "^3.1.1",
    "just-shuffle": "^3.2.1",
    "just-skewness": "^1.2.1",
    "just-snake-case": "^2.1.1",
    "just-sort-by": "^2.1.2",
    "just-split": "^2.3.1",
    "just-split-at": "^2.3.1",
    "just-squash": "^1.3.0",
    "just-standard-deviation": "^1.2.1",
    "just-tail": "^2.3.1",
    "just-template": "^1.4.0",
    "just-throttle": "^3.1.1",
    "just-truncate": "^1.4.0",
    "just-typeof": "^2.2.0",
    "just-union": "^2.3.1",
    "just-unique": "^3.4.1",
    "just-values": "^1.4.2",
    "just-variance": "^1.2.1",
    "just-zip-it": "^2.3.1"
  }
}
