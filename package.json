{
	"name": "all-of-just",
	"private": true,
	"version": "1.2.0",
	"description": "A single collection of all of Just utility functions in one single library",
	"files": [
		"*"
	],
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/puruvj/all-of-just.git"
