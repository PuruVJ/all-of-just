			"module": "./collection.js",
			"import": "./collection.js"
		},
		"./functions": {
			"types": "./functions.d.ts",
			"module": "./functions.js",
			"import": "./functions.js"
		},
		"./numbers": {
			"types": "./numbers.d.ts",
			"module": "./numbers.js",
			"import": "./numbers.js"
		},
		"./objects": {
			"types": "./objects.d.ts",
			"module": "./objects.js",
			"import": "./objects.js"
		},
		"./statistics": {
			"types": "./statistics.d.ts",
			"module": "./statistics.js",
			"import": "./statistics.js"
		},
		"./strings": {
			"types": "./strings.d.ts",
			"module": "./strings.js",
			"import": "./strings.js"
		}
	},
	"keywords": [
		"just",
		"utility",
		"lodash",
		"merge",
		"clone",
		"array",
		"tiny"
	],
	"author": "PuruVJ (@puruvjdev)",
	"license": "MIT",
