    "just-sort-by": "^1.0.0",
    "just-split": "^2.2.1",
    "just-split-at": "^2.2.0",
    "just-squash": "^1.1.0",
    "just-stdev": "^1.0.0",
    "just-tail": "^2.2.0",
    "just-template": "^1.2.0",
    "just-throttle": "^2.3.1",
    "just-truncate": "^1.2.0",
    "just-typeof": "^2.0.0",
    "just-union": "^2.2.0",
    "just-unique": "^3.3.0",
    "just-values": "^1.2.0",
    "just-variance": "^1.0.0",
    "just-zip-it": "^2.1.0"
  }
}
